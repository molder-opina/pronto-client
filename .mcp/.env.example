# Configuración de API Keys para Modelos AI
# Copia este archivo a .env.mcp y llena tus API keys

# OpenAI (GPT-4, GPT-3.5)
OPENAI_API_KEY=sk-xxxxx

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-api03-xxxxx

# MiniMax (Modelo de lenguaje chino)
MINIMAX_API_KEY=xxxxxxxx

# GLM (Zhipu AI - ChatGLM)
GLM_API_KEY=xxxxxxxx

# DeepSeek
DEEPSEEK_API_KEY=sk-xxxxx

# Configuración del servidor Chrome MCP
CHROME_PATH=/Applications/Google Chrome.app/Contents/MacOS/Google Chrome
HEADLESS=false
DEBUG=false
MCP_PORT=3000
